{
    "name": "ELO Calculator",
    "description": "Faceit ELO Calculator!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "/src/background.js"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "/src/popup.html",
        "default_icon": {
            "16": "/src/imgs/logo16.png",
            "32": "/src/imgs/logo32.png",
            "48": "/src/imgs/logo48.png",
            "128": "/src/imgs/logo128.png"
        }
    },
    "icons": {
        "16": "/src/imgs/logo16.png",
        "32": "/src/imgs/logo32.png",
        "48": "/src/imgs/logo48.png",
        "128": "/src/imgs/logo128.png"
    }
}